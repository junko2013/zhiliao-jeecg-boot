<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.im.mapper.SystemMapper">


    <select id="findByNo" resultType="org.jeecg.modules.im.entity.System">
        SELECT * FROM sys_system where no = #{no} and del_flag = 0
    </select>

</mapper>
